{
  "name": "big-two-app",
  "type": "module",
  "version": "0.0.2",
  "scripts": {
    "dev": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "test": "vitest",
    "check": "astro check",
    "db:generate": "drizzle-kit generate --config=drizzle/config.ts",
    "db:migrate": "drizzle-kit migrate  --config=drizzle/config.ts",
    "db:push": "drizzle-kit push  --config=drizzle/config.ts",
    "db:studio": "drizzle-kit studio  --config=drizzle/config.ts",
    "auth:generate": "npx @better-auth/cli generate --config src/libs/auth/better-auth.ts --output schemas/auth-schema.ts --y"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/node": "^9.0.1",
    "@astrojs/react": "^4.1.5",
    "@astrojs/tailwind": "^5.1.4",
    "@chiubaca/big-two-utils": "npm:@jsr/chiubaca__big-two-utils@^1.2.0",
    "@hono/zod-validator": "^0.4.2",
    "@paralleldrive/cuid2": "^2.2.2",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "astro": "^5.1.7",
    "better-auth": "^1.1.10",
    "better-sqlite3": "^9.6.0",
    "drizzle-kit": "^0.30.1",
    "drizzle-orm": "^0.38.3",
    "hono": "^4.6.16",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3",
    "xstate": "^5.19.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@flydotio/dockerfile": "^0.5.9",
    "@types/better-sqlite3": "^7.6.12",
    "@types/node": "^22.10.5",
    "daisyui": "^4.12.14",
    "typed-emitter": "^2.1.0",
    "vitest": "^2.1.5"
  }
}
